<?xml version="1.0" encoding="utf-8"?><odoo>    <data>       <record id="comm_form_inherit" model="ir.ui.view">           <field name="name">commission.form</field>           <field name="model">commission</field>           <field name="arch" type="xml">               <form string="commission">                   <sheet>                       <group>                           <field name="amount_from"/>                           <field name="amount_to"/>                           <field name="amount_des"/>                       </group>                   </sheet>               </form>           </field>       </record>        <record id="comm_tree_inherit" model="ir.ui.view">            <field name="name">commission.tree</field>            <field name="model">commission</field>            <field name="arch" type="xml">                <tree string="commission">                      <field name="amount_from"/>                           <field name="amount_to"/>                           <field name="amount_des"/>                </tree>            </field>        </record>        <record id="comm_action_inherit" model="ir.actions.act_window">            <field name="name">Commission</field>            <field name="type">ir.actions.act_window</field>            <field name="res_model">commission</field>            <field name="view_mode">tree,form</field>        </record>        <!-- This Menu Item Must have a parent -->        <menuitem id="sale_commission_menu_categ" name="Commissions" parent="sale.sale_menu_root" sequence="4"/>    		<menuitem id="sale_commission_menu" name="Commissions" parent="sale_commission_menu_categ" action="comm_action_inherit" sequence="2"/>    </data></odoo>