<?xml version="1.0" encoding="utf-8"?><odoo>    <data>       <!-- QWeb Reports -->       <!-- NAME & FILE_NAME should be the same --> <report id="report_warehouse"                   model="wizard.warehouse"                   report_type="qweb-pdf"                   string="Report Resupply"                   name="ninos_warehouses_report.report_resupply"                   file="ninos_warehouses_report.report_resupply"               /><template id="report_resupply">    <t t-call="web.html_container">        <t t-call="web.external_layout"><!--                <t t-foreach="docs" t-as="o">-->        <!-- <t t-call="report.external_layout"> -->            <div class="page"><!--                            <t t-set="warehouses" t-value="o.mapped('warehouses_ids')"/>--><div style="text-align:center;">                       <div style=" display:inline-block;  width:30%;                            margin:auto;margin:10px;   padding:5px;  border: 2px solid black;  " class="a"><strong>Date From:<span t-esc="datef"/></strong></div>                          <span  style="                          display:inline-block;   width:24%;   padding:5px;  border: 2px solid black; " class="a"><strong style="text-align: center;">Date To:<span t-esc="datet"/></strong></span>                </div>                        <table style="border:2px solid black" class="table table-sm table-bordered">                        <thead>                            <tr>                                <th>WareHouse</th>                                <th>Products</th>                                <th class="text-right">On Hand</th>                                <th class="text-right">Sales</th>                            </tr>                        </thead>                        <tbody> <t t-foreach="dataa" t-as="d">                                <t t-set="products" t-value="dataa[d]['products']"/>                                <t t-set="product_ids" t-value="list(products.keys())"/>                                <tr>                                    <td t-att-rowspan="dataa[d]['num_products']">                                        <t t-esc="dataa[d]['name']" />                                    </td>                                    <td>                                        <span t-esc="dataa[d]['products'][product_ids[0] ][0]" class="text-center"/>                                    </td>                                    <td class="text-right">                                        <span t-esc="dataa[d]['products'][product_ids[0] ][1]" class="text-center"/>                                    </td>                                     <td class="text-right">                                        <span t-esc="dataa[d]['products'][product_ids[0] ][2]" class="text-center"/>                                    </td>                                </tr>                                <tr t-foreach="product_ids[1:]" t-as="p">                                    <td>                                        <span t-esc="dataa[d]['products'][ p ][0]" class="text-center"/>                                    </td>                                    <td class="text-right">                                        <span t-esc="dataa[d]['products'][ p ][1]" class="text-center"/>                                    </td>                                     <td class="text-right">                                        <span t-esc="dataa[d]['products'][ p ][2]" class="text-center"/>                                    </td>                                </tr>                            </t>                        </tbody>                    </table>            </div>            </t><!--        </t>-->        </t></template>    </data></odoo>